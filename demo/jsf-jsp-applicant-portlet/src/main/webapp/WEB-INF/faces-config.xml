<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
	<application>
		<message-bundle>i18nFaces</message-bundle>
	</application>
	<lifecycle>
		<phase-listener>com.liferay.faces.util.lifecycle.DebugPhaseListener</phase-listener>
	</lifecycle>
	<managed-bean>
		<managed-bean-name>applicantBackingBean</managed-bean-name>
		<managed-bean-class>com.liferay.faces.demos.bean.ApplicantBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>applicantModelBean</property-name>
			<value>#{applicantModelBean}</value>
		</managed-property>
		<managed-property>
			<property-name>applicantViewBean</property-name>
			<value>#{applicantViewBean}</value>
		</managed-property>
		<managed-property>
			<property-name>bridgeFlash</property-name>
			<value>#{bridgeFlash}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>applicantModelBean</managed-bean-name>
		<managed-bean-class>com.liferay.faces.demos.bean.ApplicantModelBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>applicantViewBean</managed-bean-name>
		<managed-bean-class>com.liferay.faces.demos.bean.ApplicantViewBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>bridgeFlash</managed-bean-name>
		<managed-bean-class>com.liferay.faces.demos.bean.BridgeFlash</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>listModelBean</managed-bean-name>
		<managed-bean-class>com.liferay.faces.demos.bean.ListModelBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>portletPreferencesBackingBean</managed-bean-name>
		<managed-bean-class>com.liferay.faces.demos.bean.PortletPreferencesBackingBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/WEB-INF/views/portletViewMode.jspx</from-view-id>
		<navigation-case>
			<from-outcome>edit-preferences</from-outcome>
			<to-view-id>/WEB-INF/views/portletEditMode.faces?javax.portlet.faces.PortletMode=edit&amp;javax.portlet.faces.WindowState=maximized</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/WEB-INF/views/confirmation.jspx</from-view-id>
		<navigation-case>
			<from-outcome>submit-another-application</from-outcome>
			<to-view-id>/WEB-INF/views/portletViewMode.jspx</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/WEB-INF/views/portletViewMode.jspx</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/WEB-INF/views/confirmation.jspx</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
